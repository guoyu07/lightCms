{
  "name": "inhere/light-cms",
  "type": "project",
  "description": "a php micro CMS system of the php",
  "keywords": ["cms","blog"],
  "homepage": "https://github.com/inhere/lightCms",
  "license": "MIT",
  "authors": [
    {
      "name": "inhere",
      "email": "in.798@qq.com",
      "homepage": "http://www.yzone.net/"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "guzzlehttp/promises": "^1.3",
    "inhere/library": "dev-master",
    "inhere/console": "dev-master",
    "inhere/sroute": "dev-master",
    "inhere/event": "dev-master",
    "inhere/php-validate": "^2.0",
    "inhere/http": "dev-master",
    "inhere/middleware": "dev-master",
    "inhere/simple-active-record": "dev-master",
    "inhere/server": "dev-master",
    "monolog/monolog": "^1.2",
    "psr/container": "^1.0",
    "overtrue/pinyin": "^3.0",
    "windwalker/cache": "~3.0",
    "phpfastcache/phpfastcache": "^6.0",
    "league/plates": "^3.3",
    "symfony/process": "^3.3"
   },
  "require-dev": {
    "inhere/simple-print-tool": "dev-master"
  },
  "autoload": {
      "classmap": [
        "./src/system/Sys.php"
      ],
      "files": [
        "./src/system/functions.php"
      ],
      "psr-4": {
        "App\\": "./src/app/",
        "LightCms\\": "./src/system/"
      }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
   "config": {
      "secure-http": false
   }
}
