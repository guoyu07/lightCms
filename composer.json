{
  "require": {
    "php": ">=7.0.0",
    "guzzlehttp/promises": "^1.3",
    "inhere/library": "dev-master",
    "inhere/console": "dev-master",
    "inhere/sroute": "dev-master",
    "inhere/event": "dev-master",
    "inhere/php-validate": "dev-master",
    "inhere/http": "dev-master",
    "monolog/monolog": "^1.2",
    "psr/container": "^1.0",
    "overtrue/pinyin": "^3.0",
    "windwalker/cache": "~3.0"
   },
  "require-dev": {
    "inhere/simple-print-tool": "dev-master"
  },
  "autoload": {
      "classmap": [
        "./src/system/Sys.php"
      ],
      "files": [
        "./src/system/functions.php"
      ],
      "psr-4": {
        "App\\": "./src/app/",
        "LightCms\\": "./src/system/"
      }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "0": {
      "type": "git",
      "url": "https://github.com/inhere/php-event-manager.git"
    }
  },
   "config": {
      "secure-http": false
   }
}
