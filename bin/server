#!/usr/env/php
<?php

use App\Console\Controllers\ServerController;
use Inhere\Console\Application;

define('BASE_PATH', dirname(__DIR__));
define('IN_SWOOLE', true);

//require dirname(__DIR__) . '/src/boot/server.php';
require dirname(__DIR__) . '/vendor/autoload.php';

$app = new Application();

// register
$app->controller('server', ServerController::class);

// run
$status = $app->runAction('server', $app->getInput()->getCommand(), true, true);

exit((int)$status);